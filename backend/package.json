{
  "name": "clariva-bot-backend",
  "version": "1.0.0",
  "description": "Clariva Care AI Receptionist Bot - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:email": "node -r dotenv/config -r ts-node/register scripts/send-test-email.ts",
    "test:full-notifications": "node -r dotenv/config -r ts-node/register scripts/send-full-notification-test.ts"
  },
  "keywords": [
    "ai",
    "bot",
    "healthcare",
    "receptionist"
  ],
  "author": "Dr Abhishek Sahil",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.88.0",
    "axios": "^1.13.2",
    "bullmq": "^5.34.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "isomorphic-dompurify": "^2.35.0",
    "openai": "^6.14.0",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "razorpay": "^2.9.6",
    "resend": "^6.9.1",
    "twilio": "^5.10.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
